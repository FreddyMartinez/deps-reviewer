{
  "indicators": [
    "is-last-version",
    "is-same-major-version",
    "is-same-minor-version",
    "was-released-recently",
    "is-released-frequently",
    "is-downloaded-frequently",
    "is-long-living-project",
    "is-starred-repo",
    "has-open-issues",
    "has-forks",
    "has-enough-observers",
    "repo-owner-type",
    "is-healthy-repo"
  ],
  "thresholds": {
    "is-released-frequently": {
      "warningThreshold": 30,
      "alertThreshold": 90
    }
  }, 
  "conditions": {
    "mustBeOk": ["was-released-recently"],
    "maxAlerts": 2
  }
}
